{
  "name": "rosematcha.com",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "home",
    "apps/brahdb",
    "apps/garagesale"
  ],
  "scripts": {
    "dev": "npm run -w home dev",
    "prebuild": "rimraf _site",
    "build": "npm-run-all -s build:workspaces copy-static",
    "build:workspaces": "npm run build --workspaces --if-present",
    "copy-static": "cpx \"apps/bexarvoting/**\" _site/bexarvoting && cpx \"apps/friendfinder/**\" _site/friendfinder && cpx \"apps/moviemap/**\" _site/moviemap && cpx \"home/public/**\" _site && cpx \"home/src/assets/**\" _site/assets && cpx \"apps/garagesale/dist/**\" _site/garagesale && cpx \"apps/sampleballot/index.html\" _site/sampleballot && cpx \"apps/sampleballot/assets/**\" _site/sampleballot/assets && cpx \"apps/sampleballot/*.json\" _site/sampleballot",
    "lint": "npm-run-all lint:root lint:workspaces",
    "lint:root": "eslint netlify/functions",
    "lint:workspaces": "npm run lint --workspaces --if-present",
    "preview": "npm run -w home preview"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cpx": "^1.5.0",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.2",
    "@swc/core-linux-x64-gnu": "^1.12.7",
    "@swc/core-linux-x64-musl": "^1.12.7"
  }
}
